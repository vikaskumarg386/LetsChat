<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kumar.vikas.jecchat.vk.kumar.vikas.jecchat.AllBranches_Activity">

    <include layout="@layout/tool_bar"
        android:id="@+id/all_branches_toolbar" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/all_branches_recyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/all_branches_toolbar"></android.support.v7.widget.RecyclerView>

</RelativeLayout>
